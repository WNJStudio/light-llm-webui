{
  "name": "light-llm-webui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "prepare-smui-theme": "npm run smui-theme-light",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui/button": "^8.0.0-beta.3",
    "@smui/common": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/icon-button": "^8.0.0-beta.3",
    "@smui/list": "^8.0.0-beta.3",
    "@smui/switch": "^8.0.0-beta.3",
    "@smui/top-app-bar": "^8.0.0-beta.3",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "smui-theme": "^8.0.0-beta.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}
